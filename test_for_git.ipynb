    "def my_sum(*args):\n",
    "    summa = 0\n",
    "    for elem in args:\n",
    "        if isinstance(elem, int): \n",
    "            summa += elem\n",
    "    return summa\n",
    "\n",
    "print(my_sum(10, 2, -5, 66, -30))\n",
    "print(my_sum(10, 2, -5, 66, -30, 0.5))"
